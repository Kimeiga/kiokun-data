{
  "name": "kiokun-notes-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:cors\" \"npm run dev:vite\"",
    "dev:cors": "cd ../output_dictionary && python3 cors_server.py",
    "dev:vite": "node scripts/dev-with-port.js",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "cf-typegen": "wrangler types"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.5.28",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "concurrently": "^9.2.1",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.4",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "wrangler": "^4.45.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.679.0",
    "@types/dompurify": "^3.0.5",
    "better-auth": "^1.3.29",
    "dompurify": "^3.3.0",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "fflate": "^0.8.2",
    "hanzi-writer": "^3.7.3",
    "isomorphic-dompurify": "^2.30.1",
    "marked": "^16.4.1"
  },
  "type": "module"
}
